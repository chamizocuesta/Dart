void main() {
  
  final wolverine = new Heroe(nombre: 'Logan', poder: 'Regeneraci√≥n');
  print(wolverine);
}

class Heroe {
  String nombre;
  String poder;

  Heroe(
    {
      required this.nombre, 
      required this.poder
     }
  );

  @override
  String toString() {
    return 'nombre: ${this.nombre}, poder: ${this.poder}';
  }
}
